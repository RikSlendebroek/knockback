{
  "author"        : {"name": "Kevin Malakoff", "url": "https://github.com/kmalakoff"},
  "name"          : "knockback",
  "version"       : "0.18.6",
  "description"   : "Knockback.js provides Knockout.js magic for Backbone.js Models and Collections",
  "keywords"      : ["knockback", "knockbackjs", "backbone", "backbonejs", "knockout", "knockoutjs"],
  "main"          : "knockback.js",
  "private"       : true,

  "url"           : "http://kmalakoff.github.com/knockback/",
  "homepage"      : "http://kmalakoff.github.com/knockback/",
  "repository"    : {"type": "git", "url": "http://github.com/kmalakoff/knockback.git"},

  "scripts": {
    "test": "gulp test --require coffee-script/register",
    "docs": "./node_modules/.bin/codo -n Knockback.js -o doc src/.",
    "build": "gulp build --require coffee-script/register",
    "watch": "gulp watch --require coffee-script/register",
    "release": "gulp release --require coffee-script/register",
    "clean": "rm -rf lib/*"
  },
  "dependencies": {
    "underscore": ">=1.1.7",
    "backbone": ">=0.5.1",
    "knockout": ">=1.2.1"
  },
  "devDependencies": {
    "coffee-script": "*",
    "codo": "2.x",
    "gulp": "*",
    "gulp-util": "*",
    "gulp-uglify": "*",
    "gulp-rename": "*",
    "gulp-zip": "*",
    "gulp-compile-js": "*",
    "gulp-module-system": "*",
    "gulp-header": "*",
    "gulp-concat": "*",
    "gulp-mocha-phantomjs": "*",
    "gulp-shell": "*",
    "gulp-require-src": "*",
    "event-stream": "*",
    "module-bundler": "*",
    "queue-async": "*",
    "async": "*",
    "webpack": "*",
    "webpack-dev-server": "*"
  },
  "optionalDependencies": {
    "backbone-relational": "0.8.x",
    "backbone-modelref": "0.1.x",
    "lodash": "2.x",

    "backbone-orm": "0.5.x",
    "moment": "2.x",

    "backbone-associations": "0.5.x",
    "supermodel": "0.0.x"
  },
  "jam": {
    "dependencies": {
      "underscore": ">=1.1.7",
      "backbone": ">=0.5.1",
      "knockout": ">=1.2.1"
    }
  }
}
